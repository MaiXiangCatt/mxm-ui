{
  "name": "mxm-ui",
  "version": "1.0.0",
  "description": "Components library by Vue3 + TS",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s build-only",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config build/vite.umd.config.ts",
    "build-es": "vite build --config build/vite.es.config.ts",
    "release": "release-it",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@mxm-ui/components": "workspace:*",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "peerDependencies": {
    "vue": "^3.4.19"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "vite-plugin-compression2": "^2.0.1",
    "vite-plugin-dts": "^3.9.1"
  }
}
